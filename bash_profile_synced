script_dir="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
script_name=$( basename "${BASH_SOURCE[0]}" )
# Add scripts to path
export PATH=$PATH:$script_dir/scripts
# Make tmux use bash
export SHELL=/bin/bash
# PS1
export PS1="\W > "
# Easy access to this directory
export CNF=$script_dir

source $script_dir/bash_profile_local

